{{ content() }}{{ flashSession.output() }}<div class="row">    <div class="col-sm-12">        <div class="panel colourable">            <div class="panel-heading">                <span class="panel-title"><i class="fa fa-flag-checkered"></i> 重测</span>            </div>            <div class="panel-body">                <form role="form" action="" method="post" class="form-horizontal col-sm-4">                    <fieldset>                        <legend>系统重测 - Rejudge</legend>                        <div class="form-group">                            <div class="col-sm-6 col-sm-offset-3">                                <label class="checkbox-inline">                                    <input type="radio" name="type" value="SOLUTION" checked="checked" class="px"> <span class="lbl">按Solution编号</span>                                </label>                                <label class="checkbox-inline">                                    <input type="radio" name="type" value="PROBLEM" class="px"> <span class="lbl">按Problem ID</span>                                </label>                            </div>                        </div>                        <div class="form-group">                            <label class="control-label col-sm-3" for="typeid">ID/编号：</label>                            <div class="col-sm-6">                                <input type="text" class="form-control" name="typeid" id="typeid" placeholder="仅单个ID，不支持多个">                            </div>                        </div>                        <div class="form-group">                            <div class="col-sm-offset-3 col-sm-6">                                <button type="submit" class="btn btn-primary">Rejudge</button>                            </div>                        </div>                    </fieldset>                </form>            </div>        </div>    </div></div>